    "chalk": "^1.1.3",
    "interpret": "^1.2.0",
    "liftoff": "^2.5.0",
    "minimist": "^1.2.0",
    "node-plop": "~0.26.2",
    "ora": "^3.4.0",
    "v8flags": "^2.0.10"
  },
  "engines": {
    "node": ">=8.9.4"
  },
